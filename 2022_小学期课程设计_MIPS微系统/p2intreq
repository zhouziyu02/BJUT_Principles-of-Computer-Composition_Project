lw $8,($3) #in，读当前开关的值
lw $9,($2) #out 读初值，把input历史信息记录
beq $8,$9,equ  #比较$8$9，输入设备新旧两个值代表输入设备没改变，就跳转到equ计数
sw $8,($2) #判断相等不成立，输出设备改初值
sw $8,4($2) #把新的值写到秒计数的输出设备的显示寄存器上
beq $0,$0,exit
equ:lw $10,4($2)#需要秒计数的输出值取出来
addiu $10,$10,1  #秒计数，在初值的基础上+1
sw $10,4($2) #out 显示输出设备+1
exit:ori $11,$0,10
sw $11,4($1) #TC存preset
ori $12,$0,9  #ctr=1001
sw $12,($1) #TC存ctrl
eret
